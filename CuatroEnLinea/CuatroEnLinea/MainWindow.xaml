        <StackPanel HorizontalAlignment="Center" Background="Red" Height="100" VerticalAlignment="Top" Width="644">
            <Label x:Name="label1" Content="Label" Height="25" Margin="30,0,555,0"/>
            <Label x:Name="label2" Content="Label" Height="25" Margin="30,0,555,0"/>
            <Label x:Name="label3" Content="Label" Height="25" Margin="30,0,555,0"/>
        </StackPanel>
        <Grid x:Name="grilla" Background="Blue" Margin="0,100,0,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition x:Name="Columna_0" Width="*"/>
                <ColumnDefinition x:Name="Columna_1" Width="*"/>
                <ColumnDefinition x:Name="Columna_2" Width="*"/>
                <ColumnDefinition x:Name="Columna_3" Width="*"/>
                <ColumnDefinition x:Name="Columna_4" Width="*"/>
                <ColumnDefinition x:Name="Columna_5" Width="*"/>
                <ColumnDefinition x:Name="Columna_6" Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>










        </Grid>


    </Grid>
</Window>
